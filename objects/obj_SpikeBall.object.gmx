            <string>// Track ball speed
if (speed &gt; global.ballSpeed)
{
    global.ballSpeed = speed;
}

if (global.gameInProgress)
{
    // Collision with screen borders
    var top = y;
    var bottom = y + sprite_height;
    var left = x;
    var right = x + sprite_width;
    
    
    if (right &lt; sprite_width or left &gt; (room_width - sprite_width))
    {
        hspeed = -hspeed;
    }
    
    if (top &gt; (room_height - sprite_height) or bottom &lt; sprite_height)
    {
        vspeed = -vspeed;
    }
    
    
    
    // Collision with obj_Block
    var block = noone;
    if (place_meeting(x + hspeed, y, obj_Block))
    {
        block = instance_place(x + hspeed, y, obj_Block);
        hspeed = -hspeed;
    }
    if (place_meeting(x, y + vspeed, obj_Block))
    {
        block = instance_place(x, y + vspeed, obj_Block);
        vspeed = -vspeed;
    }
    
    if (block != noone)
    {
        with (block) { instance_destroy(); }
        audio_play_sound(snd_Block, 1, false);
    }
    
    
    // Collision with obj_Shield
    if (place_meeting(x + hspeed, y, obj_Shield))
    {
        hspeed = -hspeed;
        hspeed += 0.01 * hspeed;
        audio_play_sound(snd_Shield, 2, false);
    }
    if (place_meeting(x, y + vspeed, obj_Shield))
    {
        vspeed = -vspeed;
        vspeed += 0.01 * vspeed;
        audio_play_sound(snd_Shield, 2, false);
    }
    
    
    // Speed limit
    if (abs(hspeed) &gt; 30) hspeed = 20 * sign(hspeed);
    if (abs(vspeed) &gt; 30) vspeed = 20 * sign(vspeed);
}
else
{
    hspeed = 0; vspeed = 0;
}
</string>
